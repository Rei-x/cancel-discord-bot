// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cancels {
  id              Int       @id @default(autoincrement())
  timestamp       DateTime  @default(now())
  guildId         Int
  cancelledUserId Int
  whoStartedId    Int
  whoVotedForYes  Int[]
  whoVotedForNo   Int[]
}